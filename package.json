{
	"name": "proshore-todo",
	"version": "1.0.0",
	"description": "bootstrap todo application",
	"scripts": {
		"installDep:server": "cd backend && npm install",
		"installDep:client": "cd frontend && npm install",
		"start:server": "cd backend && npm start",
		"start:client": "cd frontend && npm start",
		"prestart": "npm-run-all --parallel installDep:server installDep:client",
		"start": "npm install && npm-run-all --parallel start:server start:client"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kiranojhanp/proshore-todo.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/kiranojhanp/proshore-todo/issues"
	},
	"homepage": "https://github.com/kiranojhanp/proshore-todo#readme",
	"dependencies": {
		"npm-run-all": "^4.1.5"
	}
}
